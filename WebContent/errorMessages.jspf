<%@ page import="java.util.HashMap"%>
<div id="errors">
<%	if(request.getAttribute("errors")!=null) {
		HashMap<String,String> errors = (HashMap<String,String>) request.getAttribute("errors");
		if (errors.size() > 0) { 
			for (String error: errors.values()) {%>
				<p class="error">- <%=error%></p>
			<%	} %>	
		<% 	}
	} %>
</div>